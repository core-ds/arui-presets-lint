{
    "compilerOptions": {
        "target": "es2022",
        "module": "ESNext",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react-jsx",
        "moduleResolution": "bundler",
        "allowJs": true,
        "types": ["node"],
        "outDir": "./._tsbuild"
    },
    "include": ["**/*", "**/*.mjs"]
}
